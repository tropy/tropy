<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.tropy.tpy">
    <comment>Tropy Project Database</comment>
    <comment xml:lang="en">Tropy Project Database</comment>
    <icon name="application-vnd.tropy.tpy"/>
    <magic>
      <match type="string" offset="0" value="SQLite">
        <match type="string" offset="68" value="\xda\xed\xa1\x05"/>
      </match>
    </magic>
    <glob pattern="*.tpy"/>
    <sub-class-of type="application/vnd.sqlite3"/>
  </mime-type>
  <mime-type type="application/vnd.tropy.ttp">
    <comment>Tropy Template</comment>
    <comment xml:lang="en">Tropy Template</comment>
    <icon name="application-vnd.tropy.ttp"/>
    <glob pattern="*.ttp"/>
    <sub-class-of type="application/json"/>
  </mime-type>
</mime-info>
